{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }],
    "stage-2"
  ],
  "plugins": [
    "lodash",
    "jsx-v-model",
    "transform-vue-jsx",
    "transform-runtime",
    "vue-jsx-sync",
    "transform-decorators"
  ],
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": ["transform-vue-jsx"]
    }
  }
}
